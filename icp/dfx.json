{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "rust",
      "declarations": {
        "output": "src/backend/declarations"
      }
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "declarations": {
        "output": "src/frontend/declarations"
      }
    },
    "governance_backend":{
      "candid": "src/governance_backend/governance_backend.did",
      "package": "governance_backend",
      "dependencies": ["backend"],
      "type": "rust"    
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
